{
    "name": "kitva",
    "version": "1.0.0-next.1",
    "description": "A validation kit for sveltekit",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./hook/*": {
            "import": "./dist/hook/*.js",
            "types": "./dist/hook/*.d.ts"
        },
        "./vite": {
            "import": "./dist/vite/index.js",
            "types": "./dist/vite/index.d.ts"
        },
        "./presets/ajv/*": {
            "import": "./dist/presets/ajv/*.js",
            "types": "./dist/presets/ajv/*.d.ts"
        },
        "./utils/*": {
            "import": "./dist/utils/*.js",
            "types": "./dist/utils/*.d.ts"
        },
        "./forms/*": {
            "import": "./dist/forms/*.js",
            "types": "./dist/forms/*.d.ts"
        },
        "./types": {
            "types": "./dist/types.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "hook/*": [
                "dist/hook/*.d.ts"
            ],
            "vite": [
                "dist/vite/index.d.ts"
            ],
            "presets/ajv/*": [
                "dist/presets/ajv/*.d.ts"
            ],
            "utils/*": [
                "dist/utils/*.d.ts"
            ],
            "forms/*": [
                "dist/forms/*.d.ts"
            ],
            "types": [
                "dist/types.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "*.d.ts",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "test:e2e": "DEBUG_COLORS=1 pnpm -r test",
        "test:spec": "DEBUG_COLORS=1 pnpm vitest",
        "build": "tsc",
        "dev": "tsc -w --p ./tsconfig.base.json",
        "tscheck": "tsc --noEmit",
        "pkglint": "publint"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@sinclair/typebox": "^0.28.14",
        "@sveltejs/kit": "^1.5.0",
        "@types/debug": "^4.1.8",
        "@types/node": "^20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "ajv": "^8.12.0",
        "ajv-i18n": "^4.2.0",
        "esbuild": "^0.17.19",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "esm-env": "^1.0.0",
        "prettier": "^2.8.7",
        "publint": "^0.1.12",
        "puppeteer": "^20.5.0",
        "typescript": "^5.0.2",
        "undici": "^5.22.1",
        "vite": "^4.3.0",
        "vitest": "^0.31.4",
        "zod": "^3.21.4"
    },
    "dependencies": {
        "ajv-build-tools": "^1.0.0-next",
        "debug": "^4.3.4",
        "fast-uri": "^2.2.0",
        "json-schema-to-typescript": "^13.0.1",
        "kleur": "^4.1.5",
        "rfdc": "^1.3.0",
        "svelte": "^3.59.1"
    },
    "pnpm": {
        "overrides": {
            "ajv-build-tools": "link:../ajv-build-tools"
        }
    }
}
